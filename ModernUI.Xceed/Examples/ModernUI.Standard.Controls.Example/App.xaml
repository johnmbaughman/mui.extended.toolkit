<Application x:Class="ModernUI.Xceed.AvalonDock.Example.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/FirstFloor.ModernUI;component/Assets/ModernUI.xaml" />
        <ResourceDictionary Source="/FirstFloor.ModernUI;component/Assets/ModernUI.Dark.xaml" />
        <ResourceDictionary Source="/ModernUI.Standard.Controls;component/Assets/ModernUI.Standard.Controls.xaml" />
        <ResourceDictionary Source="/ModernUI.ToggleSwitch;component/Assets/ModernUI.ToggleSwitch.xaml" />
      </ResourceDictionary.MergedDictionaries>
      <Color x:Key="AccentColor">#7777FF</Color>
    </ResourceDictionary>
  </Application.Resources>
</Application>
